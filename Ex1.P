event_2([EventH|[]]):-
	branch(EventH).

event_2([EventH|EventT]):-
	branch(EventH),
	event_2(EventT).

event_2(EventH):-
	branch(EventH).
	
branch(EventH):-
	event_schema([EventH|SchemaTail]),
	write('@@'),
	write(EventH),
	event_2(SchemaTail).

branch(EventH):-
	not(event_schema([EventH|SchemaTail])),
	write('##'),
	write(EventH),
	nl.
	
event_3(Eventdata):-
	[Eh|Et] = Eventdata,
	event_2(Eh).

