event_2([EventH|[]]):-
	write(EventH),
	nl,
	event_schema([EventH|SchemaTail]),
	append([EventH],SchemaTail,SchemaTail2),
	event_2(SchemaTail2).

event_2([EventH|EventT]):-
	write(EventH),
	nl,
	event_2(EventT),
	event_schema([EventH|SchemaTail]),
	event_2(SchemaTail).
	
event_3(Eventdata):-
	[Eh|Et] = Eventdata,
	event_schema([Eh|SchemaTail]),
	append([Eh],SchemaTail,SchemaTail2),
	event_2(SchemaTail2).

